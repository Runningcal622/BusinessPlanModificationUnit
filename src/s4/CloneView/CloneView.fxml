<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="s4.CloneView.CloneViewController">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="newYearField" promptText="Enter new year" />
            <ComboBox fx:id="statusBox" prefWidth="150.0" promptText="Edit status" />
            <Button fx:id="okButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="ok">
               <VBox.margin>
                  <Insets left="60.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
