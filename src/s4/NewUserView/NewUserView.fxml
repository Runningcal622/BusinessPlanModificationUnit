<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="s4.NewUserView.NewUserViewController">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="New User Info" />
            <TextField fx:id="usernameField" promptText="Username" />
            <TextField fx:id="passField" promptText="Password" />
            <TextField fx:id="depField" promptText="Department" />
            <ComboBox fx:id="AdminBox" prefWidth="150.0" promptText="Admin" />
            <Button fx:id="okButton" mnemonicParsing="false" text="Ok">
               <VBox.margin>
                  <Insets left="35.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
